-- Scripts:

local function JXXTT_fake_script() -- Start.LocalScript 
	local script = Instance.new('LocalScript', Start)

	Start.MouseButton1Click:Connect(function()
		Start.Text = "Loading up..."
		wait(math.random(1, 10))
		local Location = Instance.new("Part", game.Workspace)
		Location.Anchored = true
		Location.CanCollide = true
		Location.Transparency = 1
		Location.Size = Vector3.new(21.385, 1, 22.566)
		Location.Position = Vector3.new(-73.70988464355469, 48.98459243774414, 1372.9229736328125)
		local Location2 = Instance.new("Part", game.Workspace)
		Location2.Anchored = true
		Location2.Transparency = 1
		Location2.CanCollide = false
		Location2.Position = Vector3.new(-55.544776916503906, -358.1889343261719, 9495.291015625)
		Start.Text = "Loaded!"
		wait(2)
		Frame.Visible = false
		while true do
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = Location.CFrame
			wait(2)
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = Location2.CFrame
			wait(20)
		end
	end)
	
end
coroutine.wrap(JXXTT_fake_script)()
