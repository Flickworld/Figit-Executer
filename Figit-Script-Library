-- Scripts:

local function IWVO_fake_script() -- InfectionSmile.LocalScript 
	local script = Instance.new('LocalScript', InfectionSmile)

	InfectionSmile.MouseButton1Click:Connect(function()
		loadstring(game:HttpGet('https://raw.githubusercontent.com/Flickworld/Infectionous-Smile-Hack-Roblox/main/Main-Code'))()
	end)
end
coroutine.wrap(IWVO_fake_script)()
local function WUMKV_fake_script() -- PlamenHacks.LocalScript 
	local script = Instance.new('LocalScript', PlamenHacks)

	PlamenHacks.MouseButton1Click:Connect(function()
		loadstring(game:HttpGet('https://raw.githubusercontent.com/plamen6789/DoorsEntitySummonerGUI/main/EntityGUI'))()
	end)
end
coroutine.wrap(WUMKV_fake_script)()
local function SHIJJ_fake_script() -- Memes.LocalScript 
	local script = Instance.new('LocalScript', Memes)

	Memes.MouseButton1Click:Connect(function()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/stitchTheElite/luascripts/main/mmiyba3A%20tycoon.lua", true))()
	end)
end
coroutine.wrap(SHIJJ_fake_script)()
